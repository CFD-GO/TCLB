CudaDeviceFunction type_f getRho(){
	return <?R C(sum(fs)) ?>;
}
    
CudaDeviceFunction type_v getU(){
	type_f d = getRho();
	type_v u;
<?R C(PV(c("u.x","u.y")) == fs %% U) ?>
	type_v F;
	F = getF();
u.x = u.x+F.x*MagicF*0.5;
u.y = u.y+F.y*MagicF*0.5;
	u.x /= d;
	u.y /= d;
	u.z = 0;
	return u;
}

CudaDeviceFunction type_v getF() {
	type_v F;
	type_f R[9];
        if (B & NODE_BOUNDARY) {
                F.x=0;
                F.y=0;
        } else {
<?R for (i in Density$name[Density$group=="phi"]) { ?>
	if (<?%s i ?> < 0) <?%s i ?> = (<?%s i ?>+phi0)*Wetting - <?%s i ?>;
<?R } ?>

<?R  
        R = PV(c(paste("R[",1:9-1,"]",sep="")))
	Force = PV(c("F.x","F.y"));
#	gs = PV(paste("gstar[",1:9-1,"]",sep=""))
	gs = c(0,1,1,1,1,1/4,1/4,1/4,1/4);
	A = PV("MagicA")
	
	C(R==ph * ph * A + ph * ph[1] * (A*(-2)+1),float=F)
	C(Force==(R*gs) %% U)
?>
	}
	F.z=0;
	return F;
}

CudaDeviceFunction type_f getP(){
	const double a2 = 3.852462271644162;
        const double b2 = 0.1304438860971524 * 4.0 ;
        const double c2 = 2.785855170470555;
        const double t = Temperature;
        double p;
	double rho2;
	rho2 = <?R C(sum(fs)) ?>;
        switch (B & NODE_BOUNDARY) {
		case NODE_Wall:
		case NODE_MovingWall:
			rho2 = WallDensity;
	        	switch (B & NODE_ADDITIONALS) {
				case NODE_Wet:
	        	        	rho2 = WetDensity;
					break;
				case NODE_Dry:
	        	        	rho2 = DryDensity;
					break;
		        }
			break;
		case NODE_EPressure:
			rho2 = OutletDensity;
			break;
		case NODE_WPressure:
			rho2 = InletDensity;
			break;
	}
	
	p =((rho2*(-pow(b2,3)*pow(rho2,3)/64.+b2*b2*rho2*rho2/16.+b2*rho2/4.+1)*t*c2)/pow(1-b2*rho2/4.,3)-a2*rho2*rho2);
	return Magic*p;
}

CudaDeviceFunction void BounceBack()
{
     type_f uf;
<?R
  V = cbind(1:9, bounce+1)
  sel = V[,1] > V[,2]
  V = V[sel,]
  for (i in 1:nrow(V))
  {
	w = list(
           uf==f[V[i,1]],
           f[V[i,1]]==f[V[i,2]],
           f[V[i,2]]==uf
	)
	for (h in w) C(h)
   }
?>
	if ((B & NODE_BOUNDARY) == NODE_MovingWall){
		AddToMovingWallForceX(<?%s ToC(f %% U[,1]) ?>);
//		AddToMovingWallForceY(<?%s ToC(f %% U[,2]) ?>);
		AddToMovingWallForceY(f2+f5+f6);

		type_f ux = MovingWallVelocity;
		<?R
			ub=rbind(PV("ux"),0)
			C(f==ub %% t(U) * diag(tn)*12, eqstring="+=")
		?>
	}

}

