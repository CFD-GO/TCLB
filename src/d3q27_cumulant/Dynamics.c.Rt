CudaDeviceFunction void Run() {
    switch (NodeType & NODE_BOUNDARY) {
	case NODE_WVelocityTurbulent:
		WVelocityTurbulent();
		break;
	case NODE_NSymmetry:
		NSymmetry();
		break;
	case NODE_SSymmetry:
               	SSymmetry();
                break;
	case NODE_EPressure:
                EPressure();
               	break;
	case NODE_WPressure:
		WPressure();
		break;
	case NODE_SPressure:
                SPressure();
                break;
	case NODE_NPressure:
                NPressure();
                break;
	case NODE_WVelocity:
		WVelocity();
		break;
	 case NODE_NVelocity:
                NVelocity();
                break;
	 case NODE_SVelocity:
                SVelocity();
                break;
	case NODE_EVelocity:
		EVelocity();
		break;
	case NODE_Wall:
		BounceBack();
                break;
    }
    switch (NodeType & NODE_COLLISION) {
	case NODE_MRT:
		CollisionMRT();
		break;
    }
}

