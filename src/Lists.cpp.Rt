		set.id = <?%s v$Index ?>;
		set.name = "<?%s v$name ?>";
		set.unit = "<?%s v$unit ?>";
		set.isVector = <?%s if (v$vector) "true" else "false" ?>;
		set.isAdjoint = <?%s if (v$adjoint) "true" else "false" ?>;
		quantities.push_back(set);
<?R	} ?>
	}
