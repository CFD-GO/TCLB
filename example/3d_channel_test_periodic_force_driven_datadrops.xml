<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/">
        <Geometry nx="256" ny="48" nz="48">
		<MRT><Box/></MRT>
		<Wall mask="ALL">
			<Channel/>
		</Wall>
		<XYslice1><Box dz="24" nz="1"/></XYslice1>
                <XZslice1><Box dy="24" ny="1"/></XZslice1>
                <YZslice1><Box dx="128" nx="1"/></YZslice1>
	</Geometry>
	<Model>
		<Params nu="0.02"/>
		<!-- X-force and Z-force defined so that the flow will be skew wrt to the orientation axes  -->
		<!-- Channel walls are parallel to Y- plane  -->
		<Params ForceX="0.00001" ForceZ="-0.00003"/> 
	</Model>
	<VTK Iterations="10"/>
	<Log Iterations="25"/>
	<Solve Iterations="100"/>
</CLBConfig>
