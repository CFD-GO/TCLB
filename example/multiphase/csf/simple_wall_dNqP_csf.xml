<?xml version="1.0"?>
<CLBConfig output="output/" version="2.0">
	<!--
     
This sample is intended to work with:
  * d2q9_csf 
  * d3q19_csf
models.
    
-->
	<Units>
		<Params x="1m" gauge="100"/>
	</Units>
	<Geometry model="MRT" nx="100" ny="1m" predef="none">
		<MRT>
			<Box/>
		</MRT>
		<Wall name="wall" mask="ALL">
			<Box dy="-1"/>
			<Box ny="1"/>
		</Wall>
		<None name="drop">
			<Sphere dx="0.25m" dy="-0.25m" nx="0.5m" ny="0.5m"/>
		</None>
		<!--
        <WPressure name="inlet"><Inlet/></WPressure>
        <EPressure name="inlet"><Outlet/></EPressure>
    -->
	</Geometry>
	<Model>
		<Params Pressure=".0"/>
		<Params Mobility="0.001"/>
		<Params PhaseField="-0.5"/>
		<Params PhaseField-drop="0.5"/>
		<Params IntWidth=".33333"/>
		<Params Velocity="0."/>
		<Params GravitationX="0.0"/>
		<Params GravitationY="0.0"/>
		<Params Viscosity="0.1666667"/>
		<Params Viscosity_l="0.1666667"/>
		<Params SurfaceTensionDecay="0.248"/>
		<Params SurfaceTensionRate="0.0"/>
		<Params WettingAngle="0.75"/>
		<Params WallAdhesionDecay="0"/>
		<Params BrinkmanHeightInv="0"/>
		<Params PF_Advection_Switch="0"/>
	</Model>
	<Solve Iterations="10m">
		<VTK Iterations="1m"/>
		<!-- <SaveBinary Iterations="1m" />-->
	</Solve>
	<Params SurfaceTensionRate="0.01"/>
	<Params PF_Advection_Switch="1"/>
	<Solve Iterations="10000m">
		<VTK Iterations="50m"/>
		<!-- <SaveBinary Iterations="1m" />-->
	</Solve>
</CLBConfig>
