<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/">
        <Geometry nx="201" ny="201" predef="none" model="MRT">
		<MRT><Box/></MRT>

		<None name="zdrop">
            			<Sphere dx="70" nx="60" dy="70" ny="60"/>
            			<!--<Box dx="115" nx="225" dy="0" ny="5"/>-->
		</None>

	</Geometry>
	<Model>
		<Params Velocity="0.0"/>
		<Params omega="1.0"/>
		<Params	tempomega="1.0" /><!-- as omega gets defined in R preamble-->
		<Params
			Density="0.30724"
			Density-zdrop="0.02171"
			GravitationY="0.0"
			GravitationX="0.0" 
			G = "-1.0"
			/>
		<Params	T="0.075456"/><!--T_critical = 0.09432, rho_critical = 0.11911 -->
		<Params	alpha="1" /><!-- increasing alpha increases interface thickness -->
		<Params	R    ="1" /><!-- no effect on thickness or stability-->
		<Params	beta ="4" /><!-- effects density, but ratio of densities remains constant-->
		<Params	kappa="0" />   
	</Model>

<VTK/>
<Solve   Iterations="3000" output="output/">
    <VTK Iterations="100"/>
</Solve>

</CLBConfig>
